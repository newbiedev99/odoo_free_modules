<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="hide_or_show_password_assets_backend" name="Hide or Show Password Assets Backend" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/scss" href="/hide_or_show_password/static/src/css/password_eye.scss"/>
                <script type="text/javascript" src="/hide_or_show_password/static/src/js/password_eye.js"></script>
                <script type="text/javascript" src="/hide_or_show_password/static/src/js/profile.js"></script>
            </xpath>
        </template>

        <template id="hide_or_show_password_assets_frontend" name="Hide or Show Password Assets Frontend" inherit_id="web.assets_frontend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/scss" href="/hide_or_show_password/static/src/css/login_signup.scss"/>
                <script type="text/javascript" src="/hide_or_show_password/static/src/js/login_signup.js"></script>
            </xpath>
        </template>
    </data>
</odoo>